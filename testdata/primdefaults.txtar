-- in.cue --
"string": string | *"foo"
"number": number | *42
"int": int | *42
"int32": int32 | *42
"float64": float64 | *42.42
"bytes": bytes | *'hello'
"bool": bool | *false
"struct": {} | *{ field: "foo" }
"openlist": [...] | *["foo"]
"closedlist": [] | *["foo"]

-- out/exprtree/string --
"string": string | *"foo"

[*]  <string>
├── *"foo" <string>
└── <string>


-- out/exprtree/number --
"number": number | *42

[*]  <int>
├── *42 <int>
└── <number>


-- out/exprtree/int --
"int": int | *42

[*]  <int>
├── *42 <int>
└── <int>


-- out/exprtree/int32 --
"int32": int32 | *42

[*]  <int>
├── *42 <int>
└── [&]  <int>
    ├── <int>
    ├── [<=]  <number>
    │   └── 2147483647 <int>
    └── [>=]  <number>
        └── -2147483648 <int>


-- out/exprtree/float64 --
"float64": float64 | *42.42

[*]  <float>
├── *42.42 <float>
└── [&]  <number>
    ├── [<=]  <number>
    │   └── 1.7976931348... <float>
    └── [>=]  <number>
        └── -1.797693134... <float>


-- out/exprtree/bytes --
"bytes": bytes | *'hello'

[*]  <bytes>
├── *'hello' <bytes>
└── <bytes>


-- out/exprtree/bool --
"bool": bool | *false

[*]  <bool>
├── *false <bool>
└── <bool>


-- out/exprtree/struct --
"struct": {} | *{field: "foo"}

[*]  <struct>
├── *{...} <struct>
└── {...} <struct>


-- out/exprtree/openlist --
"openlist": [...] | *["foo"]

[*]  <list>
├── *[1] <list>
└── [>=0] <list<_>>


-- out/exprtree/closedlist --
"closedlist": [] | *["foo"]

[*]  <list>
├── *[1] <list>
├── [0] <list>
└── [1] <list>


